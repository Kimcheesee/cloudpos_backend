#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/macbookprom1/Documents/school/sakai-nuxt/node_modules/.pnpm/vite@4.5.0_@types+node@20.10.0_sass@1.69.5/node_modules/vite/bin/node_modules:/Users/macbookprom1/Documents/school/sakai-nuxt/node_modules/.pnpm/vite@4.5.0_@types+node@20.10.0_sass@1.69.5/node_modules/vite/node_modules:/Users/macbookprom1/Documents/school/sakai-nuxt/node_modules/.pnpm/vite@4.5.0_@types+node@20.10.0_sass@1.69.5/node_modules:/Users/macbookprom1/Documents/school/sakai-nuxt/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/macbookprom1/Documents/school/sakai-nuxt/node_modules/.pnpm/vite@4.5.0_@types+node@20.10.0_sass@1.69.5/node_modules/vite/bin/node_modules:/Users/macbookprom1/Documents/school/sakai-nuxt/node_modules/.pnpm/vite@4.5.0_@types+node@20.10.0_sass@1.69.5/node_modules/vite/node_modules:/Users/macbookprom1/Documents/school/sakai-nuxt/node_modules/.pnpm/vite@4.5.0_@types+node@20.10.0_sass@1.69.5/node_modules:/Users/macbookprom1/Documents/school/sakai-nuxt/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vite@4.5.0_@types+node@20.10.0_sass@1.69.5/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../vite@4.5.0_@types+node@20.10.0_sass@1.69.5/node_modules/vite/bin/vite.js" "$@"
fi
